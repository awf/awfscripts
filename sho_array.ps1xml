<!-- 
 ! Annotating sho_array classes for display in PowerShell
-->

<Types>
 <Type>
  <Name>ShoDoubleArray</Name>
  <Members>
    <MemberSet>
      <Name>PsStandardMembers</Name>
      <Members>
        <PropertySet>
          <Name>DefaultDisplayPropertySet</Name>
          <ReferencedProperties>
            <Name>Rows</Name>
            <Name>Columns</Name>
